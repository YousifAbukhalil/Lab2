public class PrintFullName {
    public static void main(String[] args) {
        // Replace "Your Full Name" with your actual full name
        String fullName = "Yousif Abukhalil";
        
        // Print your full name
        System.out.println("My full name is: " + fullName);
    }
}
public class PrintArrayElements {
    public static void main(String[] args) {
        // Define an array of integers
        int[] numbers = {1, 2, 3, 4, 5};
        
        // Loop through the array and print each element
        for (int i = 0; i < numbers.length; i++) {
            System.out.println("Element at index " + i + ": " + numbers[i]);
        }
    }
}
import java.util.Arrays;

public class SortStringArray {
    public static String[] sortArray(String[] inputArray) {
        // Create a copy of the input array
        String[] sortedArray = Arrays.copyOf(inputArray, inputArray.length);
        
        // Sort the copy in alphabetical order
        Arrays.sort(sortedArray);
        
        return sortedArray;
    }
    
    public static void main(String[] args) {
        String[] names = {"Alice", "Bob", "Charlie", "David"};
        
        String[] sortedNames = sortArray(names);
        
        System.out.println("Sorted names:");
        for (String name : sortedNames) {
            System.out.println(name);
        }
    }
}
